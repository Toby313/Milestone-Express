[display]

window/size/viewport_width=1224
window/size/viewport_height=548
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Milestone Express"

[editor]

version_control/plugin_name="AudioManager"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
